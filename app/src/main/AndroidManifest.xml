<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.OfficeInAndOutActivity"></activity>
        <activity android:name=".Activity.UserDetailsActivity" />
        <activity android:name=".Activity.UserHomePageActivity" />
        <activity android:name=".Activity.EmployeeDetailsActivity" />
        <activity android:name=".Activity.AddEmployeeActivity" />
        <activity android:name=".Activity.AdminControllerActivity" />
        <activity android:name=".Activity.UserSignInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>